<aura:component implements="flexipage:availableForRecordHome,force:hasRecordId" access="global">
    <aura:attribute name="recordId" type="String" description="Record to which the files should be attached" />
    <aura:attribute name="myRecordID" type="String" description="Record to which the files should be attached" />
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"/>
    <lightning:card title="Project Mobile Documents Upload" iconName="custom:custom14" class="slds-p-around_small">
        <div class="slds-p-vertical_xx-small">      
            <c:GenericMultipleFileUpload recordId="{!v.recordId}" sectionName="Promotional Images" />
        </div>
         <div class="slds-p-vertical_xx-small">      
            <c:GenericMultipleFileUpload recordId="{!v.recordId}" sectionName="Schedule Images" />
        </div>
       
        <div class="slds-p-vertical_xx-small">      
            <c:GenericMultipleFileUpload recordId="{!v.recordId}" sectionName="Project detail page image" />
        </div> 
        <div class="slds-p-vertical_xx-small">      
            <c:GenericMultipleFileUpload recordId="{!v.recordId}" sectionName="Sub image" />
        </div> 
        <div class="slds-p-vertical_xx-small">      
            <c:GenericMultipleFileUpload recordId="{!v.recordId}" sectionName="Project Brochure " />
        </div> 
        <div class="slds-p-vertical_xx-small">      
            <c:GenericMultipleFileUpload recordId="{!v.recordId}" sectionName="Project list view image " />
        </div> 
        
        <div class="slds-p-vertical_xx-small">      
            <c:GenericMultipleFileUpload recordId="{!v.recordId}" sectionName="Project Logo " />
        </div> 
    </lightning:card>
</aura:component>