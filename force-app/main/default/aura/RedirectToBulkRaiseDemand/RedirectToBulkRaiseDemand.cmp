<aura:component controller="BulkRaiseDemandController"  implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,force:hasSObjectName,forceCommunity:availableForAllPageTypes,force:lightningQuickAction" access="global">
    <aura:attribute name="sObjectName" type="String" />
    <aura:attribute name="standCustType" type="String" />
    <aura:attribute name="masterPaymentScheduleId" type="String" />
    <aura:attribute name="blockId" type="String" />
    <aura:attribute name="bookingRecords" type="Booking__c[]" />
    <aura:attribute name="matchplots" type="Object[]" default="[]"/>
    <aura:attribute name="plots" type="object[]" default="[]" access="global"/>
    <aura:attribute name="showNextCmp" type="boolean" default="false" />
    <aura:attribute name="showNextCmpCustom" type="boolean" default="false" />
    <aura:registerEvent name="raiseDemandEvent" type="c:RaiseDemandEvent"/>
    <aura:attribute name="paymentScheduleId" type="Id" />
    <aura:attribute name="grandTotalAmountCustom" type="Decimal"/>
    <aura:attribute name="Bookid" type="Id" />
    <aura:attribute name="Leadid" type="id"/>
    <aura:attribute name="isStandard" type="boolean" default = "false"/>
    <aura:attribute name="isCustome" type="boolean" default = "false"/>
    <aura:attribute name="isContinue" type="boolean" default = "false"/>
    <aura:attribute name="searchText1" type="string" default=""/>
    <aura:handler name="change" value="{!v.searchText1}" action="{!c.searchText1}"></aura:handler>
     <aura:handler name="init" value="{!this}" action="{!c.doInit}" />
    
    <lightning:select name="selectStandCust" label="Raise Based On" value="{!v.standCustType}" required="true" messageWhenValueMissing="Select type" onchange = "{!c.selectStandCus}">
        <option value="choose one">choose one...</option>
        <!--   <option value="Tower">Tower</option>-->
        <option value="Project">Project</option>
    </lightning:select>
   
</aura:component>