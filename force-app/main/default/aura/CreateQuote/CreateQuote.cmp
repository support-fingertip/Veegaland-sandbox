                            </div>
                            <div class="slds-size_4-of-8 slds-p-horizontal_small"  >
                                <lightning:input name="Common_Area_SqFt__c" type="number"  step="0.1" disabled="true" label="Common Area(SqFt)"  value="{!v.oppPlot.Common_Area_SqFt__c}" />
                            </div>
                            <div class="slds-size_4-of-8 slds-p-horizontal_small"  >
                                <lightning:input name="RERA_Area_SqFt__c" type="number"  step="0.1" disabled="true" label="RERA Area(SqFt)"  value="{!v.oppPlot.RERA_Area_SqFt__c}" />
                            </div>

                            <div class="slds-size_4-of-8 slds-p-horizontal_small"  ></div>
                            <div class="slds-size_4-of-8 slds-p-horizontal_small"  >
                                <lightning:input type="checkbox" label="Special Launch"
                                                 aura:id="checkbox1"
                                                 checked="{!v.oppPlot.Special_Launch__c}"
                                                 onchange="{!c.handleSpecialLaunchChange}"/>
                            </div>
                            <div class="slds-size_4-of-8 slds-p-horizontal_small"  >
                                <aura:if isTrue="{!v.oppPlot.Special_Launch__c}">
                                    <lightning:input name="Discount_In_Rupees__c" type="number" formatter="currency" step="0.1" disabled="false" label="Discount In Rupees"  value="{!v.oppPlot.Discount_In_Rupees__c}" onchange="{!c.handleChangeValues}" />
                                    
                                </aura:if>
                            </div>
                            <div class="slds-section slds-size_1-of-1 slds-max-small-size_1-of-1 slds-is-open">
                                <h3 class="slds-section__title slds-theme_shade">
                                    <span class="slds-truncate slds-p-horizontal_small" title="Section Title">Car Parking Information</span>
                                </h3>
                            </div>
                            <div class="slds-size_4-of-8 slds-p-horizontal_small"  >
                                <lightning:input name="No_of_Individual_Car_Parking__c" type="number" step="0" disabled="true" label="No. of Individual Car Parkings"  value="{!v.oppPlot.No_of_Individual_Car_Parking__c}" />
                            </div>
                            <div class="slds-size_4-of-8 slds-p-horizontal_small"  >
                                <lightning:input name="No_of_Back_to_Back_Car_Parking__c" type="number" step="0" disabled="true" label="No. of Back to Back Car Parkings"  value="{!v.oppPlot.No_of_Back_to_Back_Car_Parking__c}" />
                            </div>
                            <div class="slds-size_4-of-8 slds-p-horizontal_small"  >
                                <lightning:input name="Car_Parking_Charge__c" type="number" formatter="currency" step="0.01" disabled="true" label="Car Parking Charges"  value="{!v.oppPlot.Car_Parking_Charge__c}"/>
                            </div>
                            <div class="slds-size_4-of-8 slds-p-horizontal_small"  >
                                <lightning:input type="checkbox" label="Additional Car Parking Required"
                                                 aura:id="checkbox"
                                                 checked="{!v.isCheckboxChecked}"
                                                 onchange="{!c.handleCheckboxChange}"/>
                            </div>
                            <aura:if isTrue="{!v.isCheckboxChecked}">
                                <div class="slds-size_4-of-8 slds-p-horizontal_small"  >
                                    <lightning:input name="No_of_Extra_Individual_Car_Parking__c" type="number" step="0" disabled="false" label="No. of Extra Individual Car Parkings"  value="{!v.oppPlot.No_of_Extra_Individual_Car_Parking__c}" />
                                </div>
                                
                                <div class="slds-size_4-of-8 slds-p-horizontal_small"  >
                                    <lightning:input name="No_of_Extra_Back_to_Back_Car_Parking__c" type="number" step="0" disabled="false" label="No. of Extra Back to Back Car Parkings"  value="{!v.oppPlot.No_of_Extra_Back_to_Back_Car_Parking__c}" />
                                </div>
                                <div class="slds-size_4-of-8 slds-p-horizontal_small"  >
                                    <lightning:input name="Extra_car_parking_charges__c	" type="number" formatter="currency" step="0.1" disabled="false" label="Extra Car Parking Charges"  value="{!v.oppPlot.Extra_car_parking_charges__c}" onchange="{!c.handleChangeValues}" />
                                </div>
                            </aura:if>
                            <div class="slds-section slds-size_1-of-1 slds-max-small-size_1-of-1 slds-is-open">
                            </div>
                            <div class="slds-size_4-of-8 slds-p-horizontal_small"  >
                                <lightning:input name="GST__c" type="number" formatter="percent-fixed" disabled="false" label="GST"  value="{!v.oppPlot.GST__c}" onchange="{!c.handleChangeValues}" />
                            </div>
                            
                            <div class="slds-size_4-of-8 slds-p-horizontal_small"  >
                                <lightning:input name="gstAmount" type="number" formatter="currency" step="0.1" disabled="true" label="GST Amount"  value="{!v.gstAmount}" />
                            </div>
                            
                            <div class="slds-size_4-of-8 slds-p-horizontal_small"  >
                                <lightning:input name="totalFlatCost" type="number" formatter="currency" step="0.1" disabled="true" label="Total Flat Cost (inc GST)"  value="{!v.totalFlatCost}" />
                            </div>
                            
                            
