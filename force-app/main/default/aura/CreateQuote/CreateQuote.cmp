<aura:component controller="CreateQuoteController" 
                implements="force:appHostable,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes,force:lightningQuickActionwithoutHeader,force:lightningQuickAction" access="global" >
    
    <aura:attribute name="plots" type="object[]" default="[]"/>
    <aura:attribute name="plot" type="object[]" default="[]"/>
    <aura:attribute name="OppRecord" type="object[]" default="[]"/>
    <aura:attribute name="paymentplan" type="object[]" default="[]"/>
    <aura:attribute name="matchplots" type="Object[]" default="[]"/>
    <aura:attribute name="matchpaymentplan" type="Object[]" default="[]"/>
    <aura:attribute name="searchText1" type="string" default=""/>
    <aura:attribute name="searchText2" type="string" default=""/>	
    <aura:attribute name="quoteId" type="string" default=""/>
    <aura:attribute name="recordId" type="Id" />
    <aura:attribute name="oppPlot" type="Quote__c"  default="{}"/>
    <aura:attribute name="spinner" type="boolean" default="false"/>
      <aura:attribute name="showSave" type="boolean" default="false"/>
    <aura:attribute name="today" type="Date" />
    <aura:attribute name="RecTypeId" type="String"/>
    <aura:attribute name="BHKList" type="String[]"/>
    <aura:attribute name="UFList" type="String[]"/>
    <aura:attribute name="BlockList" type="String[]"/>
    <aura:attribute name="floorList" type="String[]"/>
    <aura:attribute name="propertytype" type="String[]"/>
    <aura:attribute name="BuildingList" type="String[]"/>
    <aura:attribute name="paymentTypePicklist" type="String[]" />
    <aura:attribute name="isCheckboxChecked" type="Boolean" default="false"/>
    <aura:attribute name="isSpecialLaunch" type="Boolean" default="false"/>
    <aura:attribute name="showNextCmp" type="boolean" default="false" />
    <aura:attribute name="isPopup" type="boolean" default="true" />
    <aura:attribute name="projectlist" type="String[]"/>
    <aura:attribute name="totalBasicPrice" type="Decimal"/>
    <aura:attribute name="totalFlatCost" type="Decimal"/>
    <aura:attribute name="gstAmount" type="Decimal"/>
    
    <!-- data tabel attributes -->
    <aura:attribute name="paymentSchedules" type="object[]"/>
    <aura:attribute name="mycolumns3" type="List"/>
    
    <aura:attribute name="showqute" type="Boolean" default="false"/>
    
    <!--Paymnet Schedule Attribute-->
    <aura:attribute name="projectName" type="String" />
    
    <aura:attribute name="truncatedName" type="String" />
    
    
    <aura:handler name="change" value="{!v.searchText1}" action="{!c.searchText1}"></aura:handler>
    <aura:handler name="change" value="{!v.searchText2}" action="{!c.searchText2}"></aura:handler>
    <aura:handler name="init" value="{!this}" action="{!c.doInit}"></aura:handler>
    
    <aura:html tag="style">
        .slds-modal__container{ 
        max-width: 80rem !important;
        width:80% !important;
        } 
        
        .cuf-content {
        padding: 0 0rem !important;
        }
        .slds-p-around--medium {
        padding: 0rem !important;
        }
        
        .closeIcon{
        display:none;
        }
        .slds-col{
        margin-left:5px;
        }
    </aura:html >
    
    
    <aura:if isTrue="{!Not(v.showNextCmp)}">
        <div role="dialog" tabindex="-1" aria-labelledby="header43" aura:id="Modalbox" class="slds-modal  slds-fade-in-open">
            <div class="slds-modal__container">
                <lightning:buttonIcon iconName="utility:close"
                                      onclick="{! c.closeModel }"
                                      alternativeText="close"
                                      variant="bare-inverse"
                                      class="slds-modal__close"/>
                
                <header class="slds-modal__header">
                    <h1 id="modal-heading-01" class="slds-modal__title slds-hyphenate">New Quote </h1>
                </header>
                
                
                <div class="slds-modal__content slds-p-around_medium" id="modal-content-id-1">
                    <div class="slds-grid slds-wrap slds-grid_pull-padded-x-small">
                        
                        <div class="slds-size_4-of-8 slds-max-small-size_1-of-1 slds-p-horizontal_medium"  >
                            <lightning:select aura:id="projectSelect" value="{!v.oppPlot.Project__c}" class="select" label="Project"  messageWhenValueMissing="Please select this field" disabled="false"  onchange="{!c.handleProjectChange}">
                                <option > None </option>
                                <aura:iteration items="{!v.projectlist}" var="val">
                                    <option text="{!val}" ></option>
                                </aura:iteration></lightning:select>
                        </div>
                        
                        
                        
                        <div class="slds-size_4-of-8 slds-max-small-size_1-of-1 slds-p-horizontal_small">
                            <div class="slds-combobox_container">
                                <div class="{!not(empty(v.matchplots)) ? 'slds-combobox slds-dropdown-trigger slds-dropdown-trigger_click slds-is-open' : 'slds-combobox slds-dropdown-trigger slds-dropdown-trigger_click' }" aura:id="open">
                                    <div class="slds-combobox__form-element slds-input-has-icon slds-input-has-icon_right" role="none">
                                        
                                        <lightning:input value="{!v.searchText1}" aura:id="field1" name="enter-search" label="Apartment No" type="search" required="true" id="combobox-id-1" aria-autocomplete="list" aria-controls="listbox-id-1" aria-expanded="false" aria-haspopup="listbox" autoComplete="off" role="combobox" placeholder="Search..."/>                                      
                                        
                                    </div>
                                    <div id="listbox-id-1" class="slds-dropdown slds-dropdown_length-with-icon-7 slds-dropdown_fluid" role="listbox" style ="position:relative;height:160px;">
                                        
                                        <ul class="slds-listbox slds-listbox_vertical" role="presentation">
                                            <aura:if isTrue="{!v.matchplots}">
                                                <!--<div class="{!if(not(empty(v.matchplots)), 'result-buttom','')}">-->
                                                <aura:iteration items="{!v.matchplots}" var="item">
                                                    <li role="presentation" class="slds-listbox__item">
                                                        <div id="option1" class="slds-media slds-listbox__option slds-listbox__option_entity slds-listbox__option_has-meta" role="option">
                                                            
                                                            <span class="slds-media__figure slds-listbox__option-icon">
                                                                <span class="slds-icon_container slds-icon-standard-account">
                                                                    <lightning:icon iconName="standard:user" size="xx-small"/>&nbsp;
                                                                </span>
                                                            </span>
                                                            <span class="slds-media__body">
                                                                <div class="slds-lookup__result-text" data-id="{!item.Id}" onclick="{!c.update1 }">
                                                                    <span class="slds-listbox__option-text slds-listbox__option-text_entity">{!item.Name}</span>
                                                                    <!--<span class="slds-listbox__option-meta slds-listbox__option-meta_entity">Account â€¢ Burlington, NC</span>-->
                                                                </div>
                                                            </span>
                                                        </div>
                                                    </li>
                                                </aura:iteration> 
                                            </aura:if>      
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                        
                        <aura:if isTrue="{!v.showqute}"><br/>
                            <div class="slds-section slds-size_1-of-1 slds-max-small-size_1-of-1 slds-is-open">
